{
  "name": "Brookhouser's Close all other tabs",
  "description": "Close all other tabs",
  "version": "1",
  "permissions": ["contextMenus"],
  "background": {
    "scripts": ["sample.js"]
  },
  "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Brookhouser's Close all other tabs"
    },
  "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+B"
            }
        }
    },
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
